---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }}</title>
 <subtitle>Pirátské strany</subtitle>
 <link href="{{ site.url }}/atom.xml" rel="self"/>
 <link href="{{ site.url }}/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}</id>
 <author>
   <name>{{ site.author.name }}</name>
   <email>{{ site.author.email }}</email>
 </author>

{% assign sorted_pages = (site.pages | where: "layout", "contract") %}
{% for page in sorted_pages %}
 <entry>
   <title>{{ page.title }}</title>
   <link href="{{ site.url }}{{ page.url }}"/>
   <updated>{{ page["datum podpisu"] }}</updated>
   <id>{{ site.url }}{{ page.url }}</id>
   <summary>{{ page["title"]}} Uzavřeno: {{ page["datum podpisu"] }}, Odhad nákladů: {{page["náklady"]}} Kč</summary>
   <content type="html">{{ page.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
